# 0x010f1a = 3/1 (breit, NoLabel)
# 0x010f1b = 2/1 (mittel, NoLabel)
# 0x010f19 = 1/1 (schmal, NoLabel)
highway = unclassified & (area != * | area = no) & keinrouting = yes [0x010f1a resolution 24 continue]
highway = unclassified & (area != * | area = no) & keinrouting = yes [0x010f1b resolution 23-23 continue]
highway = unclassified & (area != * | area = no) & keinrouting = yes [0x010f19 resolution 22-22]

# Nicht allgemein oeffentlich (es duerfen keine Autos fahren)
