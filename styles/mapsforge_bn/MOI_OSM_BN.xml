  <!-- bridges -->
  <rule cat="trail" e="way" k="bridge" v="yes|movable">
    <rule e="way" k="tunnel" v="no|~">
      <rule e="any" k="area" v="~">
        <rule e="way" k="highway" v="steps|footway|path" zoom-min="13">
          <line stroke="#333333" stroke-width="1.35" stroke-linecap="butt"/>
          <line stroke="#CCF8F8F8" stroke-width="1.05" stroke-linecap="butt"/>
        </rule>
      </rule>
    </rule>
    <rule e="way" k="highway" v="track" zoom-min="13">
      <line stroke="#333333" stroke-width="1.6" stroke-linecap="butt"/>
      <line stroke="#CCF8F8F8" stroke-width="1.2" stroke-linecap="butt"/>
    </rule>
    <rule e="way" k="highway" v="cycleway" zoom-min="13">
      <line stroke="#333333" stroke-width="1.45" stroke-linecap="butt"/>
      <line stroke="#CCF8F8F8" stroke-width="1.05" stroke-linecap="butt"/>
      <rule e="any" k="foot" v="ft_no|~">
        <line stroke="#3366FF" stroke-width="0.8" stroke-linecap="butt"/>
      </rule>
    </rule>
  </rule>
  <!-- highway cores -->
  <rule cat="trail" e="way" k="area" v="~|no">
    <rule e="way" k="tunnel" v="~|no">
      <rule cat="h_routes" e="any" k="*" v="*">
        <rule e="way" k="highlight" v="yes" zoom-min="13">
          <rule e="way" k="*" v="*" zoom-min="13" zoom-max="13">
            <line stroke="#8038E1E1" stroke-width="4.50"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="14" zoom-max="14">
            <line stroke="#8054E1E1" stroke-width="3.38"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="15" zoom-max="15">
            <line stroke="#8054E1E1" stroke-width="2.53"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="16" zoom-max="16">
            <line stroke="#8054E1E1" stroke-width="1.90"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="17" zoom-max="17">
            <line stroke="#8054E1E1" stroke-width="1.42"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="18">
            <line stroke="#8054E1E1" stroke-width="1.07"/>
          </rule>
        </rule>
        <rule e="way" k="hknetwork" v="lwn|uwn|rwn|nwn|iwn" zoom-min="11">
          <rule e="way" k="*" v="*" zoom-min="11" zoom-max="11">
            <line stroke="#E10600" stroke-width="1.17" stroke-dasharray="5,3" stroke-linecap="butt"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="12" zoom-max="12">
            <line stroke="#E10600" stroke-width="0.88" stroke-dasharray="5,3" stroke-linecap="butt"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="13" zoom-max="13">
            <line stroke="#80E13D38" stroke-width="3.00"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="14" zoom-max="14">
            <line stroke="#80E15854" stroke-width="2.25"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="15" zoom-max="15">
            <line stroke="#80E1726F" stroke-width="1.69"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="16" zoom-max="16">
            <line stroke="#80E1726F" stroke-width="1.27"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="17" zoom-max="17">
            <line stroke="#80E1726F" stroke-width="0.95"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="18">
            <line stroke="#80E1726F" stroke-width="0.71"/>
          </rule>
        </rule>
        <rule e="way" k="highlight" v="yes" zoom-min="11" zoom-max="12">
          <rule e="way" k="*" v="*" zoom-min="11" zoom-max="11">
            <line stroke="#00E1E1" stroke-width="1.17" stroke-dasharray="5,3" stroke-linecap="butt"/>
          </rule>
          <rule e="way" k="*" v="*" zoom-min="12" zoom-max="12">
            <line stroke="#00E1E1" stroke-width="0.88" stroke-dasharray="5,3" stroke-linecap="butt"/>
          </rule>
        </rule>
        <!-- captions -->
        <rule e="way" k="hknetwork" v="lwn|uwn|rwn|nwn|iwn" zoom-min="11">
          <caption k="ref" font-style="normal" font-size="8" fill="#CC0048" stroke="#E5FFFFFF" stroke-width="2"/>
        </rule>
        <rule e="way" k="highlight" v="yes" zoom-min="11">
          <rule e="way" k="hknetwork" v="~|no">
            <caption k="ref" font-style="normal" font-size="9" fill="#00CCCC" stroke="#E5FFFFFF" stroke-width="2"/>
          </rule>
        </rule>
        <!-- hiking nodes -->
        <rule e="node" k="hike_node" v="*">
          <rule e="any" k="*" v="*" zoom-max="12">
            <circle radius="6.4" scale-radius="false" fill="#ffffff" stroke="#009900" stroke-width="1.5"/>
            <caption k="name" display="always" priority="-14" font-style="bold" font-size="9" fill="#009900" stroke="#E5FFFFFF" stroke-width="2"/>
          </rule>
          <rule e="any" k="*" v="*" zoom-min="13" zoom-max="14">
            <circle radius="8.0" scale-radius="false" fill="#ffffff" stroke="#009900" stroke-width="2"/>
            <caption k="name" display="always" priority="-14" font-style="bold" font-size="10" fill="#009900" stroke="#E5FFFFFF" stroke-width="2"/>
          </rule>
          <rule e="any" k="*" v="*" zoom-min="15">
            <circle radius="10.4" scale-radius="false" fill="#ffffff" stroke="#009900" stroke-width="2"/>
            <caption k="name" display="always" priority="-14" font-style="bold" font-size="14" fill="#009900" stroke="#E5FFFFFF" stroke-width="2"/>
          </rule>
        </rule>
      </rule>
      <rule e="way" k="access" v="destination|private|~">
        <rule cat="h_tough" e="way" k="trail_visibility|sac_scale" v="bad|horrible|no|demanding_alpine_hiking|difficult_alpine_hiking">
          <rule e="way" k="highway" v="footway|path" zoom-min="13">
            <rule e="way" k="*" v="*" zoom-min="13" zoom-max="13">
              <line stroke="#E10600" stroke-width="0.66" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.17" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="14" zoom-max="14">
              <line stroke="#E10600" stroke-width="0.50" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.13" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="15" zoom-max="15">
              <line stroke="#E10600" stroke-width="0.37" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.10" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16" zoom-max="16">
              <line stroke="#E10600" stroke-width="0.28" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.07" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="17" zoom-max="17">
              <line stroke="#E10600" stroke-width="0.21" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.05" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="18">
              <line stroke="#E10600" stroke-width="0.16" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.04" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <!-- ;trail -->
          </rule>
        </rule>
        <rule e="way" k="trail_visibility" v="excellent|good|intermediate|~">
          <rule e="way" k="sac_scale" v="hiking|mountain_hiking|demanding_mountain_hiking|alpine_hiking|~">
            <rule e="way" k="highway" v="footway|path" zoom-min="13">
              <rule e="way" k="*" v="*" zoom-min="13" zoom-max="13">
                <line stroke="#E10600" stroke-width="0.66" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="14" zoom-max="14">
                <line stroke="#E10600" stroke-width="0.50" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="15" zoom-max="15">
                <line stroke="#E10600" stroke-width="0.37" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="16" zoom-max="16">
                <line stroke="#E10600" stroke-width="0.28" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="17" zoom-max="17">
                <line stroke="#E10600" stroke-width="0.21" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="18">
                <line stroke="#E10600" stroke-width="0.16" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <!-- ;trail -->
            </rule>
          </rule>
        </rule>
      </rule>
      <rule cat="h_acc_no" e="way" k="access" v="acc_no">
        <rule e="way" k="highway" v="footway|path" zoom-min="13">
          <rule e="way" k="*" v="*" zoom-min="13" zoom-max="13">
            <line stroke="#E10600" stroke-width="0.66" stroke-dasharray="5,3" stroke-linecap="butt"/>
