<rendertheme xmlns="http://mapsforge.org/renderTheme" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://mapsforge.org/renderTheme https://raw.githubusercontent.com/mapsforge/mapsforge/dev/resources/renderTheme-v4.xsd" version="4" map-background-outside="#00FFFFFF" map-background="#00FFFFFF">
  <stylemenu id="elmt-menu" defaultvalue="elmt-hiking" defaultlang="zh">
    <layer id="elmt-contour-c" enabled="true">
      <name lang="en" value="Contour Lines"/>
      <name lang="zh" value="等高線"/>
      <cat id="contour"/>
    </layer>
    <layer id="elmt-peak-c" enabled="true">
      <name lang="en" value="Peak"/>
      <name lang="zh" value="山頭"/>
      <cat id="peak"/>
    </layer>
    <layer id="elmt-trail-c" enabled="true">
      <name lang="en" value="Trail"/>
      <name lang="zh" value="山徑"/>
      <cat id="trail"/>
    </layer>
    <layer id="elmt-h_routes-c" enabled="true">
      <name lang="en" value="Hiking Routes"/>
      <name lang="zh" value="大眾路線"/>
      <cat id="h_routes"/>
    </layer>
    <layer id="elmt-h_acc_no-c" enabled="false">
      <name lang="en" value="Inaccess pathway"/>
      <name lang="zh" value="封閉路線"/>
      <cat id="h_acc_no"/>
    </layer>
    <layer id="elmt-hiking" visible="true">
      <name lang="en" value="Hiking v__version__"/>
      <name lang="zh" value="登山 v__version__"/>
      <overlay id="elmt-contour-c"/>
      <overlay id="elmt-peak-c"/>
      <overlay id="elmt-trail-c"/>
      <overlay id="elmt-h_routes-c"/>
      <overlay id="elmt-h_acc_no-c"/>
    </layer>
  </stylemenu>
  <!-- tunnels -->
  <rule e="way" k="tunnel" v="yes">
    <!-- highway tunnels -->
    <rule cat="footway" e="way" k="highway" v="steps|footway|path|bridleway" zoom-min="13">
      <line stroke="#30000000" dy="0.23" stroke-width="0.2" stroke-dasharray="16,3" stroke-linecap="butt"/>
      <line stroke="#80FFFFFF" dy="0.23" stroke-width="0.2" stroke-dasharray="0,16,3,0" stroke-linecap="butt"/>
      <line stroke="#30000000" dy="-0.23" stroke-width="0.2" stroke-dasharray="16,3" stroke-linecap="butt"/>
      <line stroke="#80FFFFFF" dy="-0.23" stroke-width="0.2" stroke-dasharray="0,16,3,0" stroke-linecap="butt"/>
    </rule>
  </rule>
  <!-- contour lines -->
  <rule cat="contour" e="any" k="*" v="*">
    <rule e="way" k="contour_ext" v="elevation_major" zoom-min="9" zoom-max="11">
      <rule e="way" k="ele" v="3000">
        <line stroke="#FFFFFFFF" stroke-width="0.8"/>
      </rule>
      <rule e="way" k="ele" v="2000">
        <line stroke="#EEEEEEEE" stroke-width="0.6"/>
      </rule>
      <rule e="way" k="ele" v="1000">
        <line stroke="#DDDDDDDD" stroke-width="0.4"/>
      </rule>
    </rule>
    <rule e="way" k="contour_ext" v="*" zoom-min="12" zoom-max="12">
      <rule e="way" k="ele" v="3000">
        <line stroke="#FFFFFFFF" stroke-width="0.8"/>
      </rule>
      <rule e="way" k="ele" v="2000">
        <line stroke="#EEEEEEEE" stroke-width="0.6"/>
      </rule>
      <rule e="way" k="ele" v="1000">
        <line stroke="#DDDDDDDD" stroke-width="0.6"/>
      </rule>
      <rule e="way" k="ele" v="3500">
        <line stroke="#FFFFFFFF" stroke-width="0.4"/>
      </rule>
      <rule e="way" k="ele" v="2500">
        <line stroke="#EEEEEEEE" stroke-width="0.4"/>
      </rule>
      <rule e="way" k="ele" v="1500">
        <line stroke="#DDDDDDDD" stroke-width="0.4"/>
      </rule>
      <rule e="way" k="ele" v="500">
        <line stroke="#CCCCCCCC" stroke-width="0.4"/>
      </rule>
    </rule>
    <rule e="way" k="contour_ext" v="elevation_major" zoom-min="13" zoom-max="13">
      <rule e="way" k="ele" v="3000">
        <line stroke="#FFFFFFFF" stroke-width="0.57"/>
      </rule>
      <rule e="way" k="ele" v="2000">
        <line stroke="#EEEEEEEE" stroke-width="0.43"/>
      </rule>
      <rule e="way" k="ele" v="1000">
        <line stroke="#DDDDDDDD" stroke-width="0.43"/>
      </rule>
      <rule e="way" k="ele" v="3500">
        <line stroke="#FFFFFFFF" stroke-width="0.29"/>
      </rule>
      <rule e="way" k="ele" v="2500">
        <line stroke="#EEEEEEEE" stroke-width="0.29"/>
      </rule>
      <rule e="way" k="ele" v="1500">
        <line stroke="#DDDDDDDD" stroke-width="0.29"/>
      </rule>
      <rule e="way" k="ele" v="500">
        <line stroke="#CCCCCCCC" stroke-width="0.29"/>
      </rule>
    </rule>
    <rule e="way" k="contour_ext" v="*" zoom-min="14" zoom-max="14">
      <rule e="way" k="contour_ext" v="elevation_major">
        <rule e="way" k="ele" v="3000">
          <line stroke="#FFFFFFFF" stroke-width="0.41"/>
        </rule>
        <rule e="way" k="ele" v="2000">
          <line stroke="#EEEEEEEE" stroke-width="0.41"/>
        </rule>
        <rule e="way" k="ele" v="1000">
          <line stroke="#DDDDDDDD" stroke-width="0.41"/>
        </rule>
        <rule e="way" k="ele" v="3500">
          <line stroke="#FFFFFFFF" stroke-width="0.31"/>
        </rule>
        <rule e="way" k="ele" v="2500">
          <line stroke="#EEEEEEEE" stroke-width="0.31"/>
        </rule>
        <rule e="way" k="ele" v="1500">
          <line stroke="#DDDDDDDD" stroke-width="0.31"/>
        </rule>
        <rule e="way" k="ele" v="500">
          <line stroke="#CCCCCCCC" stroke-width="0.31"/>
        </rule>
      </rule>
      <rule e="way" k="contour_ext" v="elevation_sub">
        <rule e="way" k="ele" v="3250|3750">
          <line stroke="#FFFFFFFF" stroke-width="0.20"/>
        </rule>
        <rule e="way" k="ele" v="2250|2750">
          <line stroke="#EEEEEEEE" stroke-width="0.20"/>
        </rule>
        <rule e="way" k="ele" v="1250|1750">
          <line stroke="#DDDDDDDD" stroke-width="0.20"/>
        </rule>
        <rule e="way" k="ele" v="250|750">
          <line stroke="#CCCCCCCC" stroke-width="0.20"/>
        </rule>
      </rule>
    </rule>
    <rule e="way" k="contour_ext" v="*" zoom-min="15" zoom-max="15">
      <rule e="way" k="contour_ext" v="elevation_major">
        <rule e="way" k="ele" v="3000|2000|1000">
          <line stroke="#FFFFFFFF" stroke-width="0.29"/>
        </rule>
        <rule e="way" k="ele" v="*">
          <line stroke="#EEEEEEEE" stroke-width="0.22"/>
        </rule>
      </rule>
      <rule e="way" k="contour_ext" v="elevation_medium">
        <line stroke="#DDDDDDDD" stroke-width="0.15"/>
      </rule>
    </rule>
    <rule e="way" k="contour_ext" v="*" zoom-min="16" zoom-max="16">
      <rule e="way" k="contour_ext" v="elevation_major">
        <line stroke="#FFFFFFFF" stroke-width="0.21"/>
      </rule>
      <rule e="way" k="contour_ext" v="elevation_medium">
        <line stroke="#EEEEEEEE" stroke-width="0.16"/>
      </rule>
      <rule e="way" k="contour_ext" v="elevation_sub">
        <line stroke="#DDDDDDDD" stroke-width="0.10"/>
      </rule>
    </rule>
    <rule e="way" k="contour_ext" v="*" zoom-min="17">
      <rule e="way" k="contour_ext" v="elevation_major">
        <line stroke="#FFFFFFFF" stroke-width="0.15"/>
      </rule>
      <rule e="way" k="contour_ext" v="elevation_medium">
        <line stroke="#EEEEEEEE" stroke-width="0.11"/>
      </rule>
      <rule e="way" k="contour_ext" v="elevation_sub">
        <line stroke="#DDDDDDDD" stroke-width="0.07"/>
      </rule>
      <rule e="way" k="contour_ext" v="elevation_minor">
        <line stroke="#CCCCCCCC" stroke-width="0.05"/>
      </rule>
    </rule>
    <rule e="way" k="contour_1280m" v="*" zoom-min="11" zoom-max="12">
      <rule e="way" k="contour_1280m" v="elevation_major">
        <pathText k="ele" font-size="9" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
    </rule>
    <rule e="way" k="contour_640m" v="*" zoom-min="13" zoom-max="13">
      <rule e="way" k="contour_640m" v="elevation_major">
        <pathText k="ele" font-size="11" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
    </rule>
    <rule e="way" k="contour_320m" v="*" zoom-min="14" zoom-max="14">
      <rule e="way" k="contour_320m" v="elevation_medium">
        <pathText k="ele" font-size="9" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_320m" v="elevation_major">
        <pathText k="ele" font-size="11" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
    </rule>
    <rule e="way" k="contour_320m" v="*" zoom-min="15" zoom-max="15">
      <rule e="way" k="contour_320m" v="elevation_medium">
        <pathText k="ele" font-size="11" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_320m" v="elevation_major">
        <pathText k="ele" font-size="11" font-style="bold" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
    </rule>
    <rule e="way" k="contour_160m" v="*" zoom-min="16" zoom-max="16">
      <rule e="way" k="contour_160m" v="elevation_minor">
        <pathText k="ele" font-size="9" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_160m" v="elevation_medium">
        <pathText k="ele" font-size="11" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_160m" v="elevation_major">
        <pathText k="ele" font-size="11" font-style="bold" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
    </rule>
    <rule e="way" k="contour_80m" v="*" zoom-min="17" zoom-max="17">
      <rule e="way" k="contour_80m" v="elevation_minor">
        <pathText k="ele" font-size="9" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_80m" v="elevation_medium">
        <pathText k="ele" font-size="11" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_80m" v="elevation_major">
        <pathText k="ele" font-size="11" font-style="bold" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
    </rule>
    <rule e="way" k="contour_ext" v="*" zoom-min="18">
      <rule e="way" k="contour_ext" v="elevation_sub">
        <pathText k="ele" font-size="9" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_ext" v="elevation_medium">
        <pathText k="ele" font-size="11" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_ext" v="elevation_major">
        <pathText k="ele" font-size="11" font-style="bold" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
    </rule>
  </rule>
  <!-- bridges -->
  <rule e="way" k="bridge" v="yes|movable">
    <rule e="way" k="tunnel" v="no|~">
      <rule e="any" k="area" v="~">
        <rule cat="trail" e="way" k="highway" v="steps|footway|path|bridleway" zoom-min="13">
          <line stroke="#333333" stroke-width="1.35" stroke-linecap="butt"/>
          <line stroke="#F8F8F8" stroke-width="1.05" stroke-linecap="butt"/>
        </rule>
      </rule>
    </rule>
  </rule>
  <!-- highway cores -->
  <rule e="way" k="area" v="~|no">
    <rule e="way" k="tunnel" v="~|no">
      <rule cat="trail" e="way" k="access" v="destination|private|~">
        <rule e="way" k="trail_visibility|sac_scale" v="bad|horrible|no|demanding_alpine_hiking|difficult_alpine_hiking">
          <rule e="way" k="highway" v="footway|path" zoom-min="13">
            <rule e="way" k="*" v="*" zoom-min="13" zoom-max="13">
              <line stroke="#E10600" stroke-width="0.66" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.17" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="14" zoom-max="14">
              <line stroke="#E10600" stroke-width="0.51" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.13" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="15" zoom-max="15">
              <line stroke="#E10600" stroke-width="0.37" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.09" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16" zoom-max="16">
              <line stroke="#E10600" stroke-width="0.27" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.07" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="17" zoom-max="17">
              <line stroke="#E10600" stroke-width="0.24" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.06" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="18">
              <line stroke="#E10600" stroke-width="0.22" stroke-dasharray="2,2" stroke-linecap="butt"/>
              <line stroke="#FFFFFF" stroke-width="0.06" stroke-dasharray="2,2" stroke-linecap="butt"/>
            </rule>
            <!-- ;trail -->
          </rule>
        </rule>
        <rule e="way" k="trail_visibility" v="excellent|good|intermediate|~">
          <rule e="way" k="sac_scale" v="hiking|mountain_hiking|demanding_mountain_hiking|alpine_hiking|~">
            <rule e="way" k="highway" v="footway|path" zoom-min="13">
              <rule e="way" k="*" v="*" zoom-min="13" zoom-max="13">
                <line stroke="#E10600" stroke-width="0.66" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="14" zoom-max="14">
                <line stroke="#E10600" stroke-width="0.51" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="15" zoom-max="15">
                <line stroke="#E10600" stroke-width="0.37" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="16" zoom-max="16">
                <line stroke="#E10600" stroke-width="0.27" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="17" zoom-max="17">
                <line stroke="#E10600" stroke-width="0.24" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <rule e="way" k="*" v="*" zoom-min="18">
                <line stroke="#E10600" stroke-width="0.22" stroke-dasharray="5,3" stroke-linecap="butt"/>
              </rule>
              <!-- ;trail -->
            </rule>
          </rule>
        </rule>
        <rule cat="h_acc_no" e="way" k="access" v="acc_no">
          <rule e="way" k="highway" v="footway|path" zoom-min="13">
            <rule e="way" k="*" v="*" zoom-min="13" zoom-max="13">
              <line stroke="#E10600" stroke-width="0.66" stroke-dasharray="5,3" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="14" zoom-max="14">
              <line stroke="#E10600" stroke-width="0.51" stroke-dasharray="5,3" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="15" zoom-max="15">
              <line stroke="#E10600" stroke-width="0.37" stroke-dasharray="5,3" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16" zoom-max="16">
              <line stroke="#E10600" stroke-width="0.27" stroke-dasharray="5,3" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="17" zoom-max="17">
              <line stroke="#E10600" stroke-width="0.24" stroke-dasharray="5,3" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="18">
              <line stroke="#E10600" stroke-width="0.22" stroke-dasharray="5,3" stroke-linecap="butt"/>
            </rule>
            <!-- ;trail -->
          </rule>
        </rule>
        <rule e="way" k="bicycle" v="bic_yes|bic_permissive|bic_designated">
          <line stroke="#0080FF" stroke-width="0.1" stroke-dasharray="0,1,8,2.5" stroke-linecap="butt"/>
        </rule>
        <rule e="way" k="highway" v="steps" zoom-min="13">
          <line stroke="#E10600" stroke-width="0.55" stroke-dasharray="1,1" stroke-linecap="butt"/>
          <!-- ;trail -->
        </rule>
        <!-- hiking routes -->
        <rule cat="h_routes" e="any" k="*" v="*">
          <rule e="way" k="hknetwork" v="lwn|uwn|rwn|nwn|iwn" zoom-min="11">
            <rule e="way" k="*" v="*" zoom-min="11" zoom-max="12">
              <line stroke="#E10600" stroke-width="1.0" stroke-dasharray="5,3" stroke-linecap="butt"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="13" zoom-max="13">
              <line stroke="#80E13D38" stroke-width="2.00"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="14" zoom-max="14">
              <line stroke="#80E15854" stroke-width="1.52"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="15" zoom-max="15">
              <line stroke="#80E1726F" stroke-width="1.12"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16" zoom-max="16">
              <line stroke="#80E1726F" stroke-width="0.80"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="17" zoom-max="17">
              <line stroke="#80E1726F" stroke-width="0.72"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="18">
              <line stroke="#80E1726F" stroke-width="0.64"/>
            </rule>
          </rule>
          <!-- captions -->
          <rule e="way" k="hknetwork" v="lwn|uwn|rwn|nwn|iwn" zoom-min="12">
            <caption k="ref" font-style="normal" font-size="8" fill="#CC0048" stroke="#E5FFFFFF" stroke-width="2"/>
          </rule>
          <!-- hiking nodes -->
          <rule e="node" k="hike_node" v="*">
            <rule e="any" k="*" v="*" zoom-max="12">
              <circle radius="6.4" scale-radius="false" fill="#ffffff" stroke="#009900" stroke-width="1.5"/>
              <caption k="name" display="always" priority="-14" font-style="bold" font-size="9" fill="#009900" stroke="#E5FFFFFF" stroke-width="2"/>
            </rule>
            <rule e="any" k="*" v="*" zoom-max="14" zoom-min="13">
              <circle radius="8.0" scale-radius="false" fill="#ffffff" stroke="#009900" stroke-width="2"/>
              <caption k="name" display="always" priority="-14" font-style="bold" font-size="10" fill="#009900" stroke="#E5FFFFFF" stroke-width="2"/>
            </rule>
            <rule e="any" k="*" v="*" zoom-min="15">
              <circle radius="10.4" scale-radius="false" fill="#ffffff" stroke="#009900" stroke-width="2"/>
              <caption k="name" display="always" priority="-14" font-style="bold" font-size="14" fill="#009900" stroke="#E5FFFFFF" stroke-width="2"/>
            </rule>
          </rule>
        </rule>
      </rule>
    </rule>
  </rule>
  <!-- peak -->
  <rule cat="peak" e="any" k="*" v="*">
    <rule e="node" k="natural" v="volcano">
      <rule e="node" k="*" v="*" zoom-min="13" zoom-max="14">
        <symbol id="s_volcano_small" priority="88" src="file:bn_res/s_volcano_small.svg" symbol-scaling="size" symbol-height="8" symbol-width="8"/>
        <caption k="name" symbol-id="s_volcano_small" position="above" font-style="normal" font-size="9" fill="#FFFFFF" stroke="#22000000" stroke-width="1.8"/>
        <rule e="any" k="*" v="*">
          <caption k="ele" priority="-5" symbol-id="s_peak_small" position="below" font-style="normal" font-size="8" fill="#FFFFFF" stroke="#22000000" stroke-width="1.8"/>
        </rule>
      </rule>
      <rule e="node" k="*" v="*" zoom-min="15">
        <symbol id="s_volcano" priority="88" src="file:bn_res/s_volcano.svg" symbol-scaling="size" symbol-height="12" symbol-width="12"/>
        <caption k="name" symbol-id="s_volcano" priority="88" position="above" font-style="bold" font-size="10" fill="#FFFFFF" stroke="#22000000" stroke-width="2"/>
        <caption k="ele" priority="-5" symbol-id="s_volcano" position="below" font-style="bold" font-size="10" fill="#FFFFFF" stroke="#22000000" stroke-width="2"/>
      </rule>
    </rule>
    <rule e="node" k="natural" v="peak">
      <rule e="any" k="zl" v="0" zoom-min="8" zoom-max="12">
        <symbol id="s_peak_small_m" display="always" priority="250" src="file:bn_res/s_peak_small_m.svg" symbol-scaling="size" symbol-height="18" symbol-width="18"/>
        <caption k="name" symbol-id="s_peak_small_m" priority="220" position="above" font-style="normal" font-size="9" fill="#FFFFFF" stroke="#22000000" stroke-width="1.8"/>
      </rule>
      <rule e="any" k="zl" v="1" zoom-min="11" zoom-max="12">
        <symbol id="s_peak_small_m" priority="240" src="file:bn_res/s_peak_small_m.svg" symbol-scaling="size" symbol-height="18" symbol-width="18"/>
        <caption k="name" symbol-id="s_peak_small_m" priority="210" position="above" font-style="normal" font-size="9" fill="#FFFFFF" stroke="#22000000" stroke-width="1.8"/>
