        <line stroke="#FFFFFFFF" stroke-width="0.57"/>
      </rule>
      <rule e="way" k="ele" v="2000">
        <line stroke="#EEEEEEEE" stroke-width="0.43"/>
      </rule>
      <rule e="way" k="ele" v="1000">
