<rendertheme xmlns="http://mapsforge.org/renderTheme" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://mapsforge.org/renderTheme https://raw.githubusercontent.com/mapsforge/mapsforge/dev/resources/renderTheme-v4.xsd" version="4" map-background-outside="#00FFFFFF" map-background="#00FFFFFF">
  <stylemenu id="elmt-menu" defaultvalue="elmt-hiking" defaultlang="zh">
    <layer id="elmt-contour-c" enabled="true">
      <name lang="en" value="Contour Lines"/>
      <name lang="zh" value="等高線"/>
      <cat id="contour"/>
    </layer>
    <layer id="elmt-peak-c" enabled="true">
      <name lang="en" value="Peak"/>
      <name lang="zh" value="山頭"/>
      <cat id="peak"/>
    </layer>
    <layer id="elmt-surveypoint-c" enabled="false">
      <name lang="en" value="survey point"/>
      <name lang="zh" value="標石 (基點)"/>
      <cat id="surveypoint"/>
    </layer>
    <layer id="elmt-trail-c" enabled="true">
      <name lang="en" value="Trail"/>
      <name lang="zh" value="山徑"/>
      <cat id="trail"/>
    </layer>
    <layer id="elmt-h_routes-c" enabled="true">
      <name lang="en" value="Hiking Routes"/>
      <name lang="zh" value="大眾路線"/>
      <cat id="h_routes"/>
    </layer>
    <layer id="elmt-h_acc_no-c" enabled="false">
      <name lang="en" value="Inaccess pathway"/>
      <name lang="zh" value="封閉路線"/>
      <cat id="h_acc_no"/>
    </layer>
    <layer id="elmt-h_tough" enabled="false">
      <name lang="de" value="Tough pathway"/>
      <name lang="zh" value="艱難路線"/>
      <cat id="h_tough"/>
    </layer>
    <layer id="elmt-hiking" visible="true">
      <name lang="en" value="Hiking v__version__"/>
      <name lang="zh" value="登山 v__version__"/>
      <overlay id="elmt-contour-c"/>
      <overlay id="elmt-peak-c"/>
      <overlay id="elmt-surveypoint-c"/>
      <overlay id="elmt-trail-c"/>
      <overlay id="elmt-h_routes-c"/>
      <overlay id="elmt-h_acc_no-c"/>
      <overlay id="elmt-h_tough"/>
    </layer>
  </stylemenu>
  <!-- tunnels -->
  <rule cat="trail" e="way" k="tunnel" v="yes">
    <!-- highway tunnels -->
    <rule e="way" k="highway" v="steps|footway|path" zoom-min="13">
      <line stroke="#30000000" dy="0.23" stroke-width="0.2" stroke-dasharray="16,3" stroke-linecap="butt"/>
      <line stroke="#80FFFFFF" dy="0.23" stroke-width="0.2" stroke-dasharray="0,16,3,0" stroke-linecap="butt"/>
      <line stroke="#30000000" dy="-0.23" stroke-width="0.2" stroke-dasharray="16,3" stroke-linecap="butt"/>
      <line stroke="#80FFFFFF" dy="-0.23" stroke-width="0.2" stroke-dasharray="0,16,3,0" stroke-linecap="butt"/>
    </rule>
    <rule e="way" k="highway" v="track|cycleway" zoom-min="13">
      <line stroke="#30000000" dy="0.35" stroke-width="0.2" stroke-dasharray="16,3" stroke-linecap="butt"/>
      <line stroke="#80FFFFFF" dy="0.35" stroke-width="0.2" stroke-dasharray="0,16,3,0" stroke-linecap="butt"/>
      <line stroke="#30000000" dy="-0.35" stroke-width="0.2" stroke-dasharray="16,3" stroke-linecap="butt"/>
      <line stroke="#80FFFFFF" dy="-0.35" stroke-width="0.2" stroke-dasharray="0,16,3,0" stroke-linecap="butt"/>
    </rule>
  </rule>
  <!-- contour lines -->
