      <rule e="way" k="contour_320m" v="elevation_major">
        <pathText k="ele" font-size="11" font-style="bold" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
    </rule>
    <rule e="way" k="contour_160m" v="*" zoom-min="15" zoom-max="15">
      <rule e="way" k="contour_160m" v="elevation_minor">
        <pathText k="ele" font-size="9" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_160m" v="elevation_medium">
        <pathText k="ele" font-size="11" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_160m" v="elevation_major">
        <pathText k="ele" font-size="11" font-style="bold" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
    </rule>
    <rule e="way" k="contour_80m" v="*" zoom-min="16" zoom-max="16">
      <rule e="way" k="contour_80m" v="elevation_minor">
        <pathText k="ele" font-size="9" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
      </rule>
      <rule e="way" k="contour_80m" v="elevation_medium">
        <pathText k="ele" font-size="11" fill="#DDDDDD" stroke="#222222" stroke-width="1"/>
