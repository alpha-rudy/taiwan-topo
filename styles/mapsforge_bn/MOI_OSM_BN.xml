    <rule e="way" k="contour_ext" v="*" zoom-min="13" zoom-max="13">
      <rule e="way" k="contour_ext" v="elevation_major">
        <rule e="way" k="ele" v="3000">
          <line stroke="#FFFFFFFF" stroke-width="0.41"/>
