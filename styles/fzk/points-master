# ------------------------------------------------------------
# Natur / natural:
# - vor Ortsangaben (place) verarbeiten
# ------------------------------------------------------------

# Meeresbucht. [point, closedway]
natural = bay & name = * {name "${name} ($__BAY__)"} [0x6503 resolution 24]

# Strand. [point, closedway]
natural = beach & name = * {name "${name} ($__BEACH__)"} [0x6604 resolution 24]

# Eingang zu einer Höhle. [point, closedway]
natural = cave_entrance {name "${name} ($__CAVEENTRANCE__)"} [0x6601 resolution 24]

# Klippe. [point, way, closedway]
natural = cliff & name = * {name "${name} ($__CLIFF__)"} [0x6607 resolution 23]

# Pass: Markiert die hoechste Stelle eines Gebirgspasses. [point] (s.a. natural=peak, natural=col)
(mountain_pass = yes) & name = * & ele = * {name "${name} (${ele})"} [0x6617 resolution 23]
(mountain_pass = yes) & name = *           {name "${name}"}          [0x6617 resolution 23]
(mountain_pass = yes) & ele = *            {name "N.N. (${ele})"}    [0x6617 resolution 23]
(mountain_pass = yes)                      {name "N.N."}             [0x6617 resolution 23]

# Alm, Matten: Bereich oberhalb der Baumgrenze. [point, closedway]
natural = fell & name = * {name "${name} ($__FELL__)"} [0x6600 resolution 24]

# Gebiet (auch Bergspitze), das ganzjährig mit Eis bedeckt ist. [point, closedway]
natural = glacier & name = * {name "${name} ($__GLACIER__)"} [0x650a resolution 24]

# Heide. [point, closedway]
natural = heath & name = * {name "${name} ($__HEATH__)"} [0x6600 resolution 24]

# Marsch (Feuchtgebiet, Sumpf, Moor, etc.) veraltet, siehe natural=wetland). [point, closedway]
natural = marsh & name = * {name "${name} ($__MARSH__)"} [0x6513 resolution 24]

# Schlick / Moor. [point, closedway]
natural = mud & name = * {name "${name} ($__MUD__)"} [0x6500 resolution 24]

# Bergspitze. [point]
natural = peak & name = * & ele = * {name "${name} (${ele})"} [0x6616 resolution 23]
natural = peak & name = *           {name "${name}"}          [0x6616 resolution 23]
natural = peak & ele = *            {name "N.N. (${ele})"}    [0x6616 resolution 23]
natural = peak                      {name "N.N."}             [0x6616 resolution 23]

# Boden mit Sand bedeckt, kein oder nur spärlicher Bewuchs. [point, closedway]
natural = sand & name = * {name "${name} ($__SAND__)"} [0x6600 resolution 24]

# (Loses) Geröll. [point, closedway]
natural = scree & name = * {name "${name} ($__SCREE__)"} [0x6600 resolution 24]

# Unkultiviertes Land: Unterholz / Busch. [point, closedway]
natural = scrub & name = * {name "${name} ($__SCRUB__)"} [0x6600 resolution 24]

# Quelle. [point]
natural = spring {name "${name} ($__SPRING__)" | "$__SPRING__"} [0x6511 resolution 24]

# Großer Stein, Findling. [point]
natural = stone {name "${name} ($__STONE__)" | "$__STONE__"} [0x6614 resolution 24]

# Einzelner oder signifikanter Baum. [point]
# Der Baum muss als Landmarke ausgewiesen sein.
natural = tree & denotation = landmark {name "${name} ($__TREE__)" | "$__TREE__"} [0x6619 resolution 24]

# Vulkan, entweder ruhend, erloschen oder aktiv. [point]
natural = volcano {name "${name} ($__VOLCANO__)" | "$__VOLCANO__"} [0x2c0c resolution 24]

# Wasser, Seen, Teiche usw. [point, closedway]
natural = water & name = * {name "${name} ($__WATER__)"} [0x650D resolution 24]

# Wasserfall. [point]
natural = waterfall {name "${name} ($__WATERFALL__)" | "$__WATERFALL__"} [0x6508 resolution 24]

# Feuchtgebiet wie Sumpf, Moor, Ried, Marsch, Aue, Röhricht, Watt, Salzwiese oder Mangrove.
# Naturbelassene Flächen, die Überflutungen ausgesetzt sind oder staunasse Böden aufweisen. [point, closedway]
natural = wetland & name = * {name "${name} ($__WETLAND__)"} [0x6513 resolution 24]

# Naturwald oder vollkommen unbewirtschaftete Wälder. [point, closedway]
natural = wood & name = * {name "${name} ($__WOOD__)"} [0x660A resolution 24]


# ------------------------------------------------------------
# Ortsangaben / place:
# ------------------------------------------------------------

# Kontinent. [point, closedway]
# place = continent (wird derzeit nicht unterstützt)

# Staat. [point]
# place = country (wird derzeit nicht unterstützt)

# Bundesland, Kanton. [point, closedway]
# place = state (wird derzeit nicht unterstützt)

# Region, Regierungsbezirk. [point, closedway]
# place = region (wird derzeit nicht unterstützt)

# Landkreis, Grafschaft, Bezirk. [point, closedway]
# place = county (wird derzeit nicht unterstützt)

# Megastadt (>1.000.000 Einwohner) oder Landeshauptstadt. [point, closedway]
#place = city & (population > 1000000 | capital = true) [0x0100 resolution 17]
# starting at 12 for overviewmap
place = city & (population > 1000000 | capital = true) [0x0100 resolution 12]

# Großstadt (wie von der zuständigen Behörde definiert, häufig > 100.000 Einwohner),
# Landeshauptstadt, Provinzhauptstadt. [point, closedway]
#place = city [0x0200 resolution 17]
# starting at 14 for overviewmap
place = city [0x0200 resolution 14]

# Weiler, Bauernschaft, Einöde (Ansammlung einiger Häuser). [point, closedway]
place = hamlet [0x0d00 resolution 24]

# Special treatment for Russia where place=allotments is heavily used... 
# Same treatment like above place=hamlet
place = allotments [0x0d00 resolution 24]

# "Bedeutende" Inseln werden auch in niedrigen Zoomstufen ausgewiesen.

# Insel (Nordsee, Helgoland). [point, closedway]
place = island & (name = Helgoland) [0x0600 resolution 18]

# Insel (Nordsee, Nordfriesische Inseln). [point, closedway]
place = island & (name = Amrum | name = Föhr | name = Pellworm | name = Sylt) [0x0600 resolution 18]

# Insel (Nordsee, Ostfriesische Inseln). [point, closedway]
place = island & (name = Baltrum | name = Borkum | name = Juist | name = Langeoog | name = Mellum | name = Norderney | name = Spiekeroog | name = Wangerooge) [0x0600 resolution 18]

# Insel (Ostsee). [point, closedway]
place = island & (name = Fehmarn | name = Hiddensee | name = Poel | name = Rügen | name = Ummanz | name = Usedom) [0x0600 resolution 18]

# Insel (Binnenland). [point, closedway]
place = island & (name = Harriersand) [0x0600 resolution 18]

# Insel (allgemein). [point, closedway] (Signatur analog 'hamlet')
place = island & name = * [0x0d00 resolution 23]

# An islet is any small, possibly hamlet sized piece of land that is completely surrounded by water
# and isolated from other significant landmasses. [point, closedway]
place = islet & name = * {name "${name} ($__ISLET__)"} [0x6600 resolution 24]

# Isolated_dwelling: Smallest rural settlement, smaller than a hamlet, usually just one house. [point, closedway]
place = isolated_dwelling & name = * {name "${name} ($__ISOLATEDDWELLING__)"} [0x6600 resolution 24]

# Ortsbezeichnung ohne Bezug auf Bevölkerung (z.B. "Wiesengrund", "Deutsches Eck"). [point, closedway]
#-# place = locality & name = * {name "${name} (Ortsbezeichnung)"} [0x6600 resolution 24]
place = locality & name = * & ele = * {name "${name} (${ele}) ($__LOCALITY__)"} [0x6600 resolution 23]
place = locality & name = *           {name "${name} ($__LOCALITY__)"}          [0x6600 resolution 23]
place = locality & ele = *            {name "N.N. (${ele}) ($__LOCALITY__)"}    [0x6600 resolution 23]
place = locality                      {name "N.N. ($__LOCALITY__)"}             [0x6600 resolution 23]

# Stadtteil, Stadtviertel. [point, closedway]
place = suburb [0x0b00 resolution 21]

# Stadt, Markt, selbständige Gemeinde (wie von der zuständigen Behörde definiert),
# in ländlichen Regionen > 2.000 Einwohner; in Ballungsräumen > 10.000 Einwohner. [point, closedway]
place = town [0x0600 resolution 18]

# Dorf (< 2.000 Einwohner), ehemals selbständige Gemeinde. [point, closedway]
place = village [0x0c00 resolution 21]


# ------------------------------------------
# Energieversorgung / power:
# ------------------------------------------

# Strommast: Mast für Hochspannungsüberlandleitungen. [point]
power = tower {name "${name} ($__POWERTOWER__)" | "$__POWERTOWER__"} [0x6417 resolution 24]

# Strommast für Nieder- und Mittelspannungsleitungen, oft aus Holz oder Beton. [point]
# power = pole (wird derzeit nicht unterstützt)

# Umspannwerk: bezeichnet eine Umspannanlage, die ein Höchst- oder Hochspannungs- mit einem weiteren
# Hochspannungs- oder einem Mittelspannungsnetz verbindet. [point, closedway]
power = station & name = * {name "${name} ($__POWERSTATION__)"} [0x6600 resolution 24]
power = plant & name = * {name "${name} ($__POWERSTATION__)"} [0x6600 resolution 24]

# Umspannstation, kleiner als station, verbindet ein Mittel- mit einem Niederspannungsnetz,
# z.B. Mini-Umspannwerk, Trafohäuschen oder großer Trafokasten. [point, closedway]
# Actually disabled... unclear (2013.02.014)
#power = sub_station & name = * {name "${name} ($__POWERSUBSTATION__)"} [0x6600 resolution 24]

# Windenergieanlage. [point, closedway]
power = generator & power_source = wind {name "${name} ($__WINDGENERATOR__)" | "$__WINDGENERATOR__"} [0x6419 resolution 24]
power = generator & generator:source = wind {name "${name} ($__WINDGENERATOR__)" | "$__WINDGENERATOR__"} [0x6419 resolution 24]

# Kraftwerk: dient der Erzeugung von Strom oder Wärme. [point, closedway]
power = generator & name = * {name "${name} ($__POWERGENERATOR__)"} [0x6600 resolution 24]

# Kabelverteilerschrank: in diesen Schränken werden die 400V-Stromkreise zu den Hausanschlüssen,
# Anschluss- und Übergabepunkten der regionalen Energieversorger verzweigt. Weitere Einsatzgebiete
# sind Verteilungen der kommunalen Straßenbeleuchtung oder Anschluss- und Übergabepunkte
# für Schausteller usw. [point]
# power = cable_distribution_cabinet (wird derzeit nicht unterstützt)


