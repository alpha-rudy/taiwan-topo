# Öffentliche Grünfläche (UK) (siehe auch "leisure=garden"). [point, closedway]
leisure = common & name = * {name "${name} ($__COMMON__)"} [0x2c06 resolution 24]

# Tanzen: Eine Örtlichkeit, speziell zum regelmäßigen Tanzen. [point, closedway]
leisure = dance & name = * {name "${name} ($__DANCE__)"} [0x6600 resolution 24]

# Ein Hundepark ist ein Gebiet, mit oder ohne Umzäunung, wo Hunde ohne Leine laufen dürfen. [point, closedway]
leisure = dog_park & name = * {name "${name} ($__DOGPARK__)"} [0x6600 resolution 24]

# Ein Garten ist ein Gelände, in dem Blumen und andere Pflanzen in dekorativer und strukturierter Form
# wachsen oder zu Forschungszwecken gezogen werden, zum Beispiel ein Botanischer Garten. [point, closedway]
# leisure = garden {name "${name} (Garten)" | "Garten"} [0x2c06 resolution 24]
leisure = garden & name = * {name "${name} ($__GARDEN__)"} [0x2c06 resolution 24]

# Golfanlage. [point, closedway]
leisure = golf_course & name = * {name "${name} ($__GOLFCOURSE__)"} [0x2D05 resolution 24]

# Angelstelle. [point, closedway]
# leisure = fishing (wird derzeit nicht unterstützt)

# Eislaufplatz, Eislaufbahn (in Deutschland typischerweise ein Gebäude). [point, closedway]
leisure = ice_rink & name = * {name "${name} ($__ICERINK__)"} [0x2d08 resolution 24]

# Jachthafen: Liegeplatz für Jachten und Motorboote (bzw. Boote allgemein). [point, closedway]
leisure = marina & name = * {name "${name} ($__MARINA__)"} [0x2F09 resolution 24]

# Minigolf (siehe auch Golfanlage). [point, closedway]
leisure = miniature_golf & name = * {name "${name} ($__MINIATUREGOLF__)"} [0x6600 resolution 24]

# Naturschutzgebiet (NSG), dieser Tag ist ausschließlich für Naturschutzgebiete zu nutzen.
# Andere Gebiete – wie z. B. Nationalpark, Biosphärenreservat, Landschaftsschutzgebiet, etc.
# sind von der rechtlichen Definition her und auch in der Praxis keine NSG. [point, closedway]
leisure = nature_reserve & name = * {name "${name} ($__NATURERESERVE__)"} [0x6612 resolution 24]

# Park, Grünanlage. [point, closedway]
# leisure = park {name "${name} (Park)" | "Park"} [0x2c06 resolution 24]
leisure = park & name = * {name "${name} ($__PARK__)"} [0x2c06 resolution 24]

# Spielfeld z.B. Fußball, Tennis, Handball, Hockey. [point, closedway]
leisure = pitch & name = * {name "${name} ($__PITCH__)"} [0x6600 resolution 24]

# Spielplatz für Kinder. [point, closedway]
# leisure = playground {name "${name} (Spielplatz)" | "Spielplatz"} [0x2c06 resolution 24]
leisure = playground {name "${name} ($__PLAYGROUND__)" | "$__PLAYGROUND__"} [0x640e resolution 24]

# Gibt es nur in den Commenwealth-Staaten - hier eher identisch mit landuse=village_green.
leisure = recreation_ground & name = * {name "${name} ($__RECREATIONGROUND__)"} [0x2c08 resolution 24]

# Sportzentrum, auch kommerzielle Freizeitsportanbieter, Fitnesscenter u.ä. [point, closedway]
leisure = sports_centre & name = * {name "${name} ($__SPORTSCENTRE__)"} [0x2D0A resolution 24]
leisure = sports_center & name = * {name "${name} ($__SPORTSCENTRE__)"} [0x2D0A resolution 24]

# Stadion: Eine größere Sportanlage mit permanenten, gestaffelten Sitzreihen. [point, closedway]
leisure = stadium & name = * {name "${name} ($__STADIUM__)"} [0x2c08 resolution 24]

# Laufbahn oder Rennbahn zum Beispiel für Wett-, Rad-, Hunde- oder Pferderennen. [point, way, closedway]
leisure = track & name = * {name "${name} ($__TRACK__)"} [0x2c08 resolution 24]

# Schwimmbecken. [point, closedway]
leisure = swimming_pool & name = * {name "${name} ($__SWIMMINGPOOL__)"} [0x2D09 resolution 24]

# Wasserpark: Vergnügnungsstätte mit Wasserrutschen, Schwimmbecken und Umkleiden. [point, closedway]
leisure = water_park & name = * {name "${name} ($__WATERPARK__)"} [0x2D09 resolution 24]


