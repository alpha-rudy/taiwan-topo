# Historischer Grenzstein. [point]
historic = boundary_stone & name = * {name "${name} ($__BOUNDARYSTONE__)"} [0x6600 resolution 24]

# Burg oder Schloss. [point, closedway]
historic = castle & name = * {name "${name} ($__CASTLE__)"} [0x6600 resolution 24]

# Militärisches Fort (ist gegenüber einer Burg moderner). [point, closedway]
historic = fort & name = * {name "${name} ($__FORT__)"} [0x6600 resolution 24]

# Gedenkstätte: Ähnlich wie historic=monument, aber üblicherweise kleiner. [point, closedway]
historic = memorial & name = * & memorial:type!=stolperstein & memorial:type!=plate & memorial:type!=plaque  {name "${name} ($__MEMORIAL__)"} [0x6405 resolution 24]

# Denkmal, Standbild [point, closedway]
historic = monument & name = * {name "${name} ($__MONUMENT__)"} [0x6405 resolution 24]

# Ruine. [point, closedway]
historic = ruins & military = bunker & name = * {name "${name} ($__RUINS__, $__BUNKER__)"} [0x661e resolution 24]
historic = ruins & name = *                     {name "${name} ($__RUINS__)"}         [0x661e resolution 24]

# Runestone (typically a raised stone with a runic inscription). [point]
historic=rune_stone  & name = * {name "${name} ($__RUNESTONE__)"} [0x6600 resolution 24]

# Historisches (üblicherweise christliches) Wegkreuz. [point, closedway]
historic = wayside_cross & name = * {name "${name} ($__WAYSIDECROSS__)"} [0x6600 resolution 24]

# Ein historischer Schrein, oftmals mit religiöser Darstellung. [point, closedway]
historic = wayside_shrine  & name = * {name "${name} ($__WAYSIDESHRINE__)"} [0x6405 resolution 24]

# Wrack (gesunkenes Schiff). [point, closedway]
historic = wreck & name = * {name "${name} ($__WRECK__)"} [0x1C01 resolution 24]


# --------------------------------------------
# Landnutzung / landuse:
# --------------------------------------------

# Kleingarten (Schrebergarten, Grabeland). [point, closedway]
landuse = allotments & name = * {name "${name} ($__ALLOTMENTS__)"} [0x6600 resolution 24]

# Bassin, Wasserbecken (zusammen mit basin=* für Hoch-/Regenwassersicker-/-rückhaltebecken). [point, closedway]
landuse = basin & name = * {name "${name} ($__BASIN__)"} [0x6603 resolution 24]

# Vorher bebautes Land (für Wiederbebauung vorgesehen), Industrie- und Gewerbebrache, Sanierungsgebiet, Altlast. [point, closedway]
landuse = brownfield & name = * {name "${name} ($__BROWNFIELD__)"} [0x6600 resolution 24]

# Friedhof. [closedway]
landuse = cemetery & name = * {name "${name} ($__CEMETERY__)"} [0x6403 resolution 24]

# Überwiegend Büros, Business Parks. [point, closedway]
landuse = commercial & name = * {name "${name} ($__COMMERCIAL__)"} [0x6600 resolution 24]

# Conservation land is land protected from development. [?]
landuse = conservation & name = * {name "${name} ($__CONSERVATION__)"} [0x6600 resolution 24]

# Baugebiet. [point, closedway]
landuse = construction & name = * {name "${name} ($__CONSTRUCTION__)"} [0x6600 resolution 24]

# Landwirtschaftlich genutzte Flächen mit Gebäuden wie Häuser, Unterkünften, Ställen, Fahrzeughallen, Futterspeichern usw.
# Ebenso die Flächen zwischen den Gebäuden, sowie nahe Bäume, Hecken und Grünflächen. [closedway]
landuse = farmyard & name = * {name "${name} ($__FARMYARD__)"} [0x6600 resolution 24]

# Forst, landwirtschaftlich genutzter Wald (siehe auch natural=wood). [point, closedway]
landuse = forest & name = * {name "${name} ($__FOREST__)"} [0x660A resolution 24]

# Rasenfläche (vorzugsweise innerorts). [point, closedway]
landuse = grass & name = * {name "${name} ($__GRASSAREA__)"} [0x6600 resolution 24]

# Bauerwartungsland, unerschlossenes, für Bebauung vorgesehenes Land (keine vorherige Bebauung). [point, closedway]
landuse = greenfield & name = * {name "${name} ($__GREENFIELD__)"} [0x6600 resolution 24]

# Großes Hafengebiet / -areal: The harbour site (land area, not area of water), mostly fenced or
# outlined by hedge, barrier or access restriction is tagged on land using "landuse=harbour". [?]
landuse = harbour & name = * {name "${name} ($__HARBOUR__)"} [0x650B resolution 24]

# Gewerbe-/Industriegebiet (überwiegend Werkstätten, Fabriken oder Lagerhallen). [point, closedway]
landuse = industrial & name = * {name "${name} ($__INDUSTRIALAREA__)"} [0x6600 resolution 24]

# Deponie, Aufschüttung, Müllhalde. [point, closedway]
landuse = landfill & name = * {name "${name} ($__LANDFILL__)"} [0x6600 resolution 24]

# Wiese (meist außerorts; nicht für öffentliche Zwecke). [point, closedway]
landuse = meadow & name = * {name "${name} ($__MEADOW__)"} [0x6600 resolution 24]

# Militärisch genutztes Gebiet (Kasernen, Truppenübungsplätze, use.). [point, closedway]
landuse = military & name = * {name "${name} ($__MILITARY__)"} [0x640B resolution 24]

# Obstplantage, Obstgarten, Gemüseanbau, Anpflanzung von Bäumen oder Sträuchern zur Lebensmittelproduktion. [point, closedway]
landuse = orchard & name = * {name "${name} ($__ORCHARD__)"} [0x6600 resolution 24]

# Steinbruch, Grube (Kies, Ton, ...), oberflächlicher Abbau von Rohstoffen (Tagebau). [point, closedway]
landuse = quarry & name = * {name "${name} ($__QUARRY__)"} [0x6600 resolution 24]

# Erholungsgebiet. [point, closedway]
landuse = recreation_ground & name = * {name "${name} ($__RECREATIONGROUND__)"} [0x6600 resolution 24]

# Speicherbecken (auch Talsperren; siehe auch landuse=basin). [point, closedway]
landuse = reservoir & name = * {name "${name} ($__RESERVOIR__)"} [0x6500 resolution 24]

# Wohngebiet. [point, closedway]
landuse = residential & name = * {name "${name} ($__RESIDENTIAL__)"} [0x6600 resolution 24]

# Einkaufszentrum oder überwiegend Einzelhandelsgeschäfte. [point, closedway]
landuse = retail & name = * {name "${name} ($__RETAIL__)"} [0x2E04 resolution 24]

# Meerwassersaline, angelegter oder natürlicher Salzgarten, der das Salz durch Verdunstung von Meerwasser freigibt. [closedway]
landuse = salt_pond & name = * {name "${name} ($__SALTPOND__)"} [0x6500 resolution 24]

# Dorfanger/-wiese (innerorts). [point, closedway]
landuse = village_green & name = * {name "${name} ($__VILLAGEGREEN__)"} [0x6600 resolution 24]

# Weinberg, Wengert. [point, closedway]
landuse = vineyard & name = * {name "${name} ($__VINEYARD__)"} [0x6600 resolution 24]


# --------------------------------------------
# Militär / military:
# --------------------------------------------

# Militärflugplatz: Ein Ort, an dem militärische Luftfahrzeuge starten und landen können. [point, closedway]
military = airfield & name = * {name "${name} ($__AIRFIELD__)"} [0x640B resolution 24]

# Gebäude: Kaserne, Unterkünfte und Arbeitsgebäude für Soldaten. [point, closedway]
military = barracks & name = * {name "${name} ($__BARRACKS__)"} [0x640B resolution 24]

# Gebäude: Bunker, Gebäude mit sehr dicken Mauern, die auch schweren Beschuss stand halten. [point, closedway]
military = bunker & ruins = yes & name = * {name "${name} ($__RUINS__, $__BUNKER__)"} [0x661e resolution 24]
military = bunker & name = *               {name "${name} ($__MILITARYBUNKER__)"}   [0x661d resolution 24]

# Gefahrenzone, in der man sich nicht aufhalten sollte. [point, closedway]
military = danger_area & name = * {name "${name} ($__MILITARYDANGERAREA__)"} [0x640B resolution 24]

# Waffenübungsplatz (z.B. ein Schießstand, aber auch größere Truppenübungsplätze). [point, closedway]
military = range & name = * {name "${name} ($__MILITARYRANGE__)"} [0x640B resolution 24]

# Marinestützpunkt. [point, closedway]
military = naval_base & name = * {name "${name} ($__NAVALBASE__)"} [0x640B resolution 24]


# --------------------------------------------
# Geologie / geological:
# --------------------------------------------

# Fossillagerstaette. Sedimentgesteine die eine außergewöhnlich hohe Anzahl von Fossilien und
# Fossilienvielfalt und/oder durch eine besonders vollständige Fossilerhaltung auszeichnen. [point, closedway]
geological = palaeontological_site & name = * {name "${name} ($__PALEONTOLOGICALSITE__)"} [0x6600 resolution 24]

# A outcrop site is a place where is absent in the coverage of alteration due to exogenous agents
# or vegetation, and where, therefore, the rock macroscopic surfaces by being directly accessible
# to analysis. [point, closedway]
geological = outcrop & name = * {name "${name} ($__OUTCROP__)"} [0x6600 resolution 24]


