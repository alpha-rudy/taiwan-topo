# --------------------------------------------

# Local authorities, administrative and supervising institutions that are not related to state government. [point, closedway]
office = administrative & name = * {name "${name} ($__ADMINISTRATIVE__)"} [0x3007 resolution 24]

# Job-Center / Arbeitsvermittlung. [point, closedway]
office = employment_agency & name = * {name "${name} ($__EMPLOYMENTAGENCY__)"} [0x3007 resolution 24]

# Büro einer Regierung / Behörde / Regierungseinrichtung. [point, closedway]
office = government & name = * {name "${name} ($__GOVERNMENT__)"} [0x3007 resolution 24]

# Büro einer Nichtregierungsorganisation. [point, closedway]
office = ngo & name = * {name "${name} ($__NGO__)"} [0x3007 resolution 24]

# Büro oder Sitz einer halbstaatlichen Organisation. [point, closedway]
office = quango & name = * {name "${name} ($__QUANGO__)"} [0x3007 resolution 24]


# ------------------------------------------------------------
# Barrieren / barrier:
# ------------------------------------------------------------

# Großer, stabiler Block, der nur mit schwerem Gerät oder großer Anstrengung versetzt werden kann. [point]
barrier = block {add access = no; add bicycle = yes; add foot = yes} [0x1211 resolution 24]

# Ein Poller, Pfosten oder eine Bake auf einem Weg oder einer Straße, der die Durchfahrt verhindert. [point, way]
barrier = bollard {add access = no; add bicycle = yes; add foot = yes} [0x1208 resolution 24]

# Weiderost, "Bovi-Stop" (überfahrbarer Gitterrost, für Huftiere unüberwindbar). [point]
# barrier = cattle_grip (wird derzeit nicht unterstützt)

# Absperrkette. [point]
# barrier = chain (wird derzeit nicht unterstützt)

# Beschränkung der Fahrraddurchfahrt. Gängige Bezeichnungen sind Drängelgitter oder Umlaufsperre.
# Typischerweise zwei in den Weg hineinreichende versetzte Metallbügel, die das Durchfahren mit
# Fahrrädern oder Motorrädern unmöglich machen oder zumindest verzögern, die Passage von Fußgängern
# erlauben. In Deutschland oft am Ende von Radwegen um Radfahrer nicht direkt auf die vorfahrtsberechtigte
# Straße fahren zu lassen oder das Befahren des Radweges mit motorisierten Fahrzeugen zu verhindern.
# Sehr häufig finden sich solche Umlaufsperren auch an Fußgängerüberwegen über Bahnstrecken. [point]
barrier = cycle_barrier {add access = no; add foot = yes} [0x1209 resolution 24]

# Entrance: Standard Tag zur Beschreibung, dass an dieser Stelle ein Durchlass durch die Barriere ist. [point]
# barrier = entrance (wird derzeit nicht unterstützt)

# Tor, Schranke oder sonstige Absperrung, die geöffnet werden kann. [point, way]
barrier = gate [0x1207 resolution 24]

# Horse_stile: Ein Zaunübertritt für Pferde ermöglicht es Fußgängern und Pferden eine Lücke im Zaun
# zu überqueren, aber verhindert es nahezu, dass Motorräder und Vieh diese überqueren. [point]
# barrier = horse_stile (wird derzeit nicht unterstützt)

# Schlagbaum. [point]
barrier = lift_gate [0x1210 resolution 24]

# Ausfalltor, Durchgangsschleuse - Ein Durchgang mit einem inneren und einem äußeren Tor. [point]
# barrier = sally_port (wird derzeit nicht unterstützt)

# Stile: Tritt über Zaun. [point]
# barrier = stile (wird derzeit nicht unterstützt)

# Drehkreuz: läßt Personen einzeln ein- oder austreten. Häufig als Ausgang von Parks oder Freibädern. [point]
# barrier = turnstile (wird derzeit nicht unterstützt)


# ------------------------------------------------------------
# Besonderheiten:
# ------------------------------------------------------------

# Grabstellen bedeutender Persönlichkeiten (d.h. Personen mit Wikipedia-Eintrag)
# cemetery = grave & name = * & (wikipedia:de = * | wikipedia = * | website ~ 'wikipedia') {name "${name} (Ruhestaette)"} [0x6405 resolution 24]
cemetery = grave & name = * & (wikipedia:de = * | wikipedia = * | website = *) {name "${name} ($__GRAVE__)"} [0x6405 resolution 24]


# ------------------------------------------------
# Needed Include Lines for making 
# old mkgmap styles (pre r2906)
# fully compatible with mkgmap r2906 and higher
# ------------------------------------------------
# add the following lines to your lines file
 <finalize>
# deactivated #include 'compat_points';
name=* { name '${name}' }
mkgmap:default_name=* { name '${mkgmap:default_name}' }
mkgmap:display_name=* { addlabel '${mkgmap:display_name|subst:;=>/}' }

mkgmap:label:1!=* & ref=* { addlabel '${ref|part:;:1}' }
mkgmap:label:1!=* & int_ref=* { addlabel '${int_ref|part:;:1}' }
mkgmap:label:1!=* & nat_ref=* { addlabel '${nat_ref|part:;:1}' }
mkgmap:label:1!=* & reg_ref=* { addlabel '${reg_ref|part:;:1}' }

ref=*     { addlabel '${ref|part:;:1}';     addlabel '${ref|part:;:2}';     addlabel '${ref|part:;:3}'     }
int_ref=* { addlabel '${int_ref|part:;:1}'; addlabel '${int_ref|part:;:2}'; addlabel '${int_ref|part:;:3}' }
nat_ref=* { addlabel '${nat_ref|part:;:1}'; addlabel '${nat_ref|part:;:2}'; addlabel '${nat_ref|part:;:3}' }
reg_ref=* { addlabel '${reg_ref|part:;:1}'; addlabel '${reg_ref|part:;:2}'; addlabel '${reg_ref|part:;:3}' }

include "indexsearch";
